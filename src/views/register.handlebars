<link rel="stylesheet" href="/css/usersStyles.css" />
<h2>Registrate</h2>
<div class="user register" action>
    <form method="post">
        <label for="first_name">Nombre</label>
        <input name="first_name" type="text" required pattern="^[A-Z][a-z]+( [A-Z][a-z]+)*$"/>
        <label for="last_name">Apellido</label>
        <input name="last_name" type="text" required pattern="^[A-Z][a-z]+( [A-Z][a-z]+)*$"/>
        <label for="email">Correo electrónico</label>
        <input name="email" type="email" required />
        <label for="age">Edad</label>
        <input name="age" type="number" required/>
        <label for="password">Contraseña</label>
        <input name="password" type="password" required pattern="^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#$%^&*_=+-]).{8,120}$"/>
        <input type="submit" name="" id="" value="Enviar"/>
    </form>
</div>